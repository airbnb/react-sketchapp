    "react-native": "^0.42.3",
    "react-primitives": "^0.6.0",
    "react-sketchapp": "^2.0.0",
    "react-test-renderer": "^16.3.2"
  },
  "devDependencies": {
    "@skpm/builder": "^0.4.0"
  }
