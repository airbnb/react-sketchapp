    "react-native": "^0.42.3",
    "react-primitives": "^0.3.4",
    "react-sketchapp": "^2.0.0",
    "react-test-renderer": "^16.3.2"
  },
  "devDependencies": {
    "extract-text-webpack-plugin": "^2.1.0",
    "nwb": "^0.15.6",
    "@skpm/builder": "^0.4.0"
  }
